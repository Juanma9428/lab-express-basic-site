<h1>POKEMONS</h1>
<div class="container">
  <h1>Players</h1>
  <div class="row">
      {{#each pokemons}}
        <div class="col-md-4">
  <div class="card">
    <img
      class="card-img-top"
      src="{{this.image}}"
      alt="{{this.name}}"
    />
    <div class="card-body">
      <h5 class="card-title">{{this.name}}</h5>
    
    </div>
  </div>
</div>

      {{/each}}
  </div>
</div>